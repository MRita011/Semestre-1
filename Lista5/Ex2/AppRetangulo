import java.util.Scanner;

public class AppRetangulo 
{
    public static void main (String args[]) {
        Scanner in = new Scanner(System.in);
        // Classe    objeto       novo objeto (constructor)  
        Retangulo retangulo1 = new Retangulo();

        System.out.println("Digite uma nova altura: ");
        double altura = in.nextDouble();

        System.out.println("Digite uma nova base: ");
        double base = in.nextDouble();

        retangulo1.setAltura(altura);
        retangulo1.setBase(base);
        System.out.println(retangulo1.toString());
    }
}
